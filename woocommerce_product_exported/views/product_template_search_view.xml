<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="product_template_search_woocommerce_product_exported_view" model="ir.ui.view">
        <field name="name">product.template.woocommerce.product.exported</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_search_view"/>
        <field name="arch" type="xml">
            <xpath expr="//filter[@name='consumable']" position="after">
                <separator/>
                <filter string="Exported to WooCommerce" name="exported_to_woocommerce"
                        domain="[('is_woocommerce_exported','=',True)]"/>
                <filter string="Not Exported to WooCommerce" name="not_exported_to_woocommerce"
                        domain="[('is_woocommerce_exported','=',False)]"/>
                <group expand="1" string="Group By">
                    <filter string="Exported to WooCommerce" domain="[]"
                            context="{'group_by':'is_woocommerce_exported'}"/>
                </group>
            </xpath>
        </field>
    </record>

</odoo>
