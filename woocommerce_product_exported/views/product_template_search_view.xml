<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_product_template_search" model="ir.ui.view">
        <field name="name">product.template.search</field>
        <field name="model">product.template</field>
        <field name="arch" type="xml">
            <search string="Search Product">
                <field name="name" />
                <field name="is_woocommerce_exported" />
                <separator />
                <filter string="Exported to WooCommerce" name="exported_to_woocommerce" domain="[('is_woocommerce_exported','=',True)]"/>
                <filter string="Not Exported to WooCommerce" name="not_exported_to_woocommerce" domain="[('is_woocommerce_exported','=',False)]"/>
                <group expand="1" string="Group By">
                    <filter string="Exported to WooCommerce" domain="[]" context="{'group_by':'is_woocommerce_exported'}"/>
                </group>
            </search>
        </field>
    </record>
</odoo>
